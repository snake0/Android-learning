<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BD_TranslucentTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="dialogstyle">
        <item name="android:backgroundDimAmount">0.4</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="pub_react_video_popup_toast_anim">
        <item name="android:windowEnterAnimation">@android:anim/fade_in</item>
        <item name="android:windowExitAnimation">@android:anim/fade_out</item>
    </style>
    <style name="pub_react_video_style_dialog_progress">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@style/pub_react_video_popup_toast_anim</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
    <style name="spider_AppBaseTheme">
    </style>
    <style name="spider_AppSplash">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    <style name="spider_ButtonWhite">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">@color/spider_button_white_txcolor_selector</item>
        <item name="android:background">@drawable/spider_button_white_bg_selector</item>
    </style>
    <style name="spider_StyleCheckBox">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:background">@drawable/spider_btn_check_label_background</item>
        <item name="android:button">?android:attr/listChoiceIndicatorMultiple</item>
    </style>
    <style name="spider_StyleEditText">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">@color/spider_hint_color_selector</item>
        <item name="android:background">@drawable/spider_editview_bg</item>
    </style>
    <style name="spider_StyleListView">
        <item name="android:background">#ffffffff</item>
        <item name="android:fadingEdgeLength">10dp</item>
        <item name="android:listSelector">@drawable/spider_theme_list_item_bg</item>
        <item name="android:scrollingCache">false</item>
        <item name="android:cacheColorHint">#0000</item>
        <item name="android:divider">#ffe1e1e1</item>
        <item name="android:dividerHeight">1dp</item>
        <item name="android:overScrollMode">never</item>
    </style>
    <style name="spider_StyleTextView">
        <item name="android:textSize">14dp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textColorHint">@color/spider_hint_color_selector</item>
    </style>
    <style name="spider_Theme_App">
        <item name="android:buttonStyle">@style/spider_ButtonWhite</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:checkboxStyle">@style/spider_StyleCheckBox</item>
        <item name="android:editTextStyle">@style/spider_StyleEditText</item>
        <item name="android:listViewStyle">@style/spider_StyleListView</item>
        <item name="android:textViewStyle">@style/spider_StyleTextView</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/spider_checkbox_selector</item>
    </style>
    <style name="spider_Theme_App_Translucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="spider_Theme_App_Translucent_QRN">
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
    <style name="spider_Theme_App_dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <style name="spider_Theme_Float">
        <item name="android:buttonStyle">@style/spider_ButtonWhite</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:checkboxStyle">@style/spider_StyleCheckBox</item>
        <item name="android:editTextStyle">@style/spider_StyleEditText</item>
        <item name="android:listViewStyle">@style/spider_StyleListView</item>
        <item name="android:textViewStyle">@style/spider_StyleTextView</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/spider_checkbox_selector</item>
        <item name="android:windowSoftInputMode">adjustUnspecified|stateVisible|adjustResize</item>
        <item name="android:colorBackgroundCacheHint">@android:color/transparent</item>
    </style>
    <style name="spider_Window_Translate_Animation">
        <item name="android:windowEnterAnimation">@anim/spider_side_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/spider_side_out_to_bottom</item>
    </style>
</resources>
